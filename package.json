{
  "name": "@conjoon/create-conjoon",
  "type": "module",
  "version": "0.0.15",
  "description": "A scaffolding utility to help you instantly set up a functional conjoon app.",
  "scripts": {
    "create-conjoon": "create-conjoon",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch && npm run postversion",
    "release:minor": "standard-version --release-as minor && npm run postversion",
    "release:major": "standard-version --release-as major && npm run postversion",
    "postversion": "git push && git push --tags"
  },
  "bin": "bin/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conjoon/create-conjoon.git"
  },
  "keywords": [
    "conjoon",
    "email",
    "webmail",
    "scaffolding",
    "extjs",
    "sencha",
    "javascript",
    "php"
  ],
  "author": "Thorsten Suckow-Homberg <thorsten@suckow-homberg.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conjoon/create-conjoon/issues"
  },
  "homepage": "https://github.com/conjoon/create-conjoon",
  "dependencies": {
    "@docusaurus/logger": "^2.0.0-beta.21",
    "@l8js/l8": "^0.7.4",
    "commander": "^9.3.0",
    "fs-extra": "^10.1.0",
    "prompts": "^2.4.2",
    "shelljs": "^0.8.5",
    "supports-color": "^9.2.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "standard-version": "^9.3.1"
  }
}
